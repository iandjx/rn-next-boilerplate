{
    "name": "kitten",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "ios:11:dev": "run-s env:dev && react-native run-ios --simulator='iPhone 11 Pro'",
        "start": "react-native start --reset-cache",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "pod": "cd ios pod install --repo-update && cd ..",
        "pod-m1": "cd ios && arch -x86_64 pod install --repo-update && cd ..",
        "clean:ios": "rm -rf ios/build",
        "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
        "build:android": "yarn clean:android && cd android && ./gradlew assembleRelease && cd ..",
        "xcode": "xed ios",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "clean": "run-p clean:*",
        "rename-app": "npx react-native-rename \"Name App\" -b com.nameapp",
        "env:dev": "node scripts/set-environment.js development",
        "env:stage": "node scripts/set-environment.js staging",
        "env:prod": "node scripts/set-environment.js production",
        "convert2Webp": "sh ./scripts/cwebp.sh",
        "gen-svg": "npx svgicon-create"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.15.8",
        "@react-native-community/masked-view": "^0.1.11",
        "@react-native-community/netinfo": "^6.0.2",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/stack": "^6.0.7",
        "@reduxjs/toolkit": "^1.6.1",
        "@shopify/polaris-tokens": "^4.0.0",
        "@shopify/restyle": "^1.4.0",
        "axios": "^0.21.4",
        "i18next": "^21.0.1",
        "lodash": "^4.17.21",
        "react": "17.0.2",
        "react-i18next": "^11.12.0",
        "react-native": "0.65.1",
        "react-native-fast-image": "^8.5.6",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-localize": "^2.1.4",
        "react-native-reanimated": "^2.2.2",
        "react-native-safe-area-context": "^3.3.2",
        "react-native-screens": "^3.7.2",
        "react-native-svg": "^12.1.1",
        "react-native-vector-icons": "^8.1.0",
        "react-redux": "^7.2.5",
        "redux": "^4.1.1",
        "redux-persist": "^6.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@td-design/svgicon-cli": "^1.1.4",
        "@testing-library/jest-native": "^4.0.2",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/react-native": "^7.2.0",
        "@types/faker": "^5.5.8",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.173",
        "@types/react-native": "^0.65.0",
        "@types/react-test-renderer": "^17.0.1",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.14.0",
        "faker": "^5.5.3",
        "husky": "^7.0.2",
        "jest": "^26.6.3",
        "lint-staged": "^11.1.2",
        "metro-react-native-babel-preset": "^0.66.0",
        "msw": "^0.35.0",
        "npm-run-all": "^4.1.5",
        "react-native-codegen": "^0.0.7",
        "react-native-rename": "^2.9.0",
        "react-test-renderer": "17.0.2",
        "ts-jest": "^27.0.5",
        "typescript": "^4.4.3"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "jest": {
        "preset": "react-native",
        "setupFiles": [
            "./jest.setup.js"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}